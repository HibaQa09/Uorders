<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="MyPanel" prefHeight="600.0" prefWidth="336.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.uorders.loginController">
   <children>
      <ImageView fx:id="Panel" fitHeight="633.0" fitWidth="336.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Assets/qtis3.png" />
         </image>
      </ImageView>
      <Button fx:id="button_login" layoutX="74.0" layoutY="525.0" mnemonicParsing="false" onAction="#log" prefHeight="47.0" prefWidth="188.0" styleClass="btn" stylesheets="@../style.css" text="Login" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="SignUp" layoutX="222.0" layoutY="24.0" mnemonicParsing="false" onMouseClicked="#signup" prefHeight="31.0" prefWidth="100.0" style="-fx-background-color: 251;" stylesheets="@../style.css" text="Sign up" textAlignment="CENTER" textFill="WHITE" textOverrun="CENTER_WORD_ELLIPSIS" underline="true">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="forgot" layoutX="89.0" layoutY="482.0" mnemonicParsing="false" onMouseClicked="#forgot1" prefHeight="31.0" prefWidth="158.0" style="-fx-background-color: 251;" stylesheets="@../style.css" text="I forgot my password" textAlignment="CENTER" textFill="WHITE" textOverrun="CENTER_WORD_ELLIPSIS" underline="true">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Text fx:id="label_login" fill="WHITE" layoutX="98.0" layoutY="302.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Log in">
         <font>
            <Font name="Arial Black" size="42.0" />
         </font>
      </Text>
      <TextField fx:id="Email" layoutX="74.0" layoutY="352.0" prefHeight="31.0" prefWidth="235.0" promptText="Email">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <PasswordField fx:id="Password" layoutX="74.0" layoutY="428.0" prefHeight="31.0" prefWidth="235.0" promptText="Password">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
   </children>
</AnchorPane>
